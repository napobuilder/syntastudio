{
  "name": "syntastudio-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "dev:ecommerce": "npm run dev --workspace=@synta/ecommerce",
    "dev:web-apps": "npm run dev --workspace=@synta/web-apps",
    "build:ecommerce": "npm run build --workspace=@synta/ecommerce",
    "build:web-apps": "npm run build --workspace=@synta/web-apps",
    "install:ecommerce": "npm install --workspace=@synta/ecommerce",
    "install:web-apps": "npm install --workspace=@synta/web-apps"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
